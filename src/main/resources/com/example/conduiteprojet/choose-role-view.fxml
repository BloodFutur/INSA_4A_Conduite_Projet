<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import java.lang.String?>
<?import javafx.geometry.Insets?>



<StackPane prefWidth="231.0" xmlns:fx="http://javafx.com/fxml"
           fx:controller="com.example.conduiteprojet.ChooseRoleController">
<padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
</padding>
<StackPane>
    <VBox spacing="10.0">
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="109.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="189.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <Label text="Username:" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
            <Label text="Password:" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
            <TextField fx:id="textFieldUsername" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
            <TextField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="1"/>

            <Label text="First Name:" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
            <Label text="Last Name:" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
            <TextField fx:id="textFieldFirstname" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
            <TextField fx:id="textFieldLastname" GridPane.columnIndex="1" GridPane.rowIndex="3"/>

            <ChoiceBox fx:id="roleBox" GridPane.columnIndex="0" GridPane.rowIndex="4">
                <String fx:value="Patient"/>
                <String fx:value="Benevole"/>
            </ChoiceBox>
        </GridPane>


        <StackPane prefHeight="-1.0" prefWidth="-1.0">
            <Button fx:id="validateButton" alignment="CENTER" defaultButton="true" mnemonicParsing="false"
                    text="Validate" StackPane.alignment="CENTER_RIGHT" onAction="#onValidateButtonClick"/>
        </StackPane>
    </VBox>
</StackPane>
</StackPane>